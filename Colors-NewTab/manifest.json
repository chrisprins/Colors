{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Colors - New Tab",
    "short_name": "Colors",
    "description": "A multicolored 'paint splashes' New Tab.",
    "version": "0.2.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon96.png",
        "128": "icon128.png"
    },
    "permissions": [
        "background",
        "chrome://favicon/",
        "topSites",
        "tabs",
        "geolocation",
        "unlimitedStorage",
        "activeTab",
        "storage",
        "bookmarks",
        "management"
	],
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "offline_enabled": true
}
